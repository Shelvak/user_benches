<br>
<br>

<h1>Total: <%= distance_of_time_in_words(@total) %></h1>
<br>
<br>

<table class="table table-stripped">
  <thead>
    <tr>
      <th>User</th>
      <th>Taken time</th>
      <th>Reported at</th>
    </tr>
  </thead>

  <tbody>
    <% @user_benches.each do |user_bench| %>
      <tr>
        <td><%= user_bench.user %></td>
        <td><%= distance_of_time_in_words(user_bench.bench_time) %></td>
        <td><%= l user_bench.created_at, format: '%Y/%m/%d %H:%M' %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= paginate @user_benches %>
